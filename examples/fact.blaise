program Fact;

var n:Integer;

function Factorial(n:Integer):Integer
  begin
		if n = 0 then 
			result := 1
		else
    	result := n * Factorial(n-1)
  end;
procedure ZeroToTen(n:Integer,f:Fun(Integer):Integer)
	var t : Integer;
  begin
		t := n;
		while t > 0 do
		begin
  		writeln("Res: fact(",t,")=",f(t));
			t := t-1
		end
  end;
begin
	readln(n);
  ZeroToTen(n, Factorial)
end.
